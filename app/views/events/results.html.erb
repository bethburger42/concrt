<h1>Concert Results Page</h1>

<%= link_to '&larr; Back'.html_safe, events_path, class: 'btn btn-primary back' %>

<h2>By Zip Code: <%= @results["Events"][0]["Venue"]["ZipCode"] %></h2>
<h2><%= @results["Events"][0]["Date"] %></h2>
<% @results["Events"].each do |e| %>
<div class="well">
  <h3><%= e["Artists"].each do |a| %>
      <%= a["Name"] %>
    <% end %>
  </h3>
    <h3><%= e["Venue"]["Name"] %>, <%= e["Venue"]["City"] %>, <%= e["Venue"]["StateCode"] %>
  </h3> 
  <h4> <a href="<%= e["TicketUrl"] %>"><%= e["TicketUrl"] %></a></h4>

  </div>


<% end %>